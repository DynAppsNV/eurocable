<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="add_fixed_sale_phrase_form" model="ir.ui.view">
        <field name="name">xx.add.fixed.sale.phrase.form</field>
        <field name="model">xx.add.fixed.sale.phrase</field>
        <field name="arch" type="xml">
            <form string="Add Fixed Sale Phrase">
                <sheet>
                    <group>
                        <field name="sale_order_ids" widget="many2many_tags" />
                        <field name="fixed_sale_phrase_ids" widget="many2many_tags" />
                    </group>
                </sheet>
                <footer>
                    <button
                        name="add_fixed_sale_phrase"
                        string="Confirm"
                        default_focus="1"
                        data-hotkey="q"
                        type="object"
                        class="btn-primary"
                    />
                </footer>
            </form>
        </field>
    </record>
</odoo>
